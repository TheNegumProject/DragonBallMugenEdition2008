[State 0, 1]
type = ChangeAnim
triggerall = Var(1) = 3
trigger1 = Anim != 6460 && Anim != 6465
trigger2 = Anim = 6465 && AnimTime = 0 ;Turn anim over
value = 6460

[State 0, 1]
