triggerall = Var(1) = 4
trigger1 = Anim != 3654 && Anim != 3659
trigger2 = Anim = 3659 && AnimTime = 0 ;Turn anim over
value = 3654



[State 0, 2]
type = VelSet
trigger1 = Time = 0
y = 0

[State 0, 3] ;Stop moving if low velocity or 4 ticks pass
