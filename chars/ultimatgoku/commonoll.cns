[Statedef 0]
type = S
physics = S

[State 0, 1]
type = ChangeAnim
triggerall = Var(1) = 0
trigger1 = Anim != 0 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0 ;Turn anim over
value = 0

[State 0, 1]
